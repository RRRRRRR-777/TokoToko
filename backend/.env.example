# アプリケーション設定
ENVIRONMENT=development
PORT=8080

# データベース設定
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=tekutoko
DB_SSLMODE=disable

# Firebase設定
FIREBASE_CREDENTIALS_PATH=./credentials/firebase-adminsdk.json
FIREBASE_PROJECT_ID=tekutoko-ios

# ログ設定
LOG_LEVEL=debug
LOG_FORMAT=text

# pgAdmin設定（オプション）
PGADMIN_EMAIL=admin@tekutoko.com
PGADMIN_PASSWORD=admin
PGADMIN_PORT=5050
